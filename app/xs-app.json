{
  "welcomeFile": "app/webapp/index.html",
  "logout": {
    "logoutEndpoint": "/auth/logout",
    "logoutPage": "https://1660cf5etrial-btp-course-risk-management.cfapps.us10-001.hana.ondemand.com/app/webapp/index.html"
  },
  "login": {
    "callbackEndpoint": "https://1660cf5etrial-btp-course-risk-management.cfapps.us10-001.hana.ondemand.com/app/webapp/index.html"
  },
  "routes": [
    {
      "source": "^/app/(.*)$",
      "target": "$1",
      "localDir": "./risk-management",
      "authenticationType": "xsuaa",
      "cacheControl": "no-cache, no-store, must-revalidate"
    },
    {
      "source": "^/appconfig/",
      "localDir": ".",
      "cacheControl": "no-cache, no-store, must-revalidate"
    },
    {
      "source": "^/(.*)$",
      "target": "$1",
      "authenticationType": "xsuaa",
      "destination": "srv-api",
      "csrfProtection": true
    }
  ]
}